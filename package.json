{
  "private": true,
  "name": "@softwareventures/promise",
  "version": "0.0.0-development",
  "description": "Pure functional utilities for Promises",
  "keywords": [
    "lambda",
    "TypeScript",
    "functional",
    "Promise"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/promise",
  "bugs": "https://github.com/softwareventures/promise/issues",
  "repository": "github:softwareventures/promise",
  "license": "ISC",
  "scripts": {
    "fix": "eslint . --fix && prettier --write .",
    "lint": "eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "3.6.1",
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "1.1.1",
    "@softwareventures/tsconfig": "5.1.0",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.25.0",
    "prettier": "2.2.1",
    "semantic-release": "17.4.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
